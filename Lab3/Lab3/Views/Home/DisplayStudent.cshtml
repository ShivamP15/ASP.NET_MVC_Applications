@{
    Layout = "_Layout";
    ViewData["Title"] = "Display Student";
}
@model Lab3.Models.Student

<h1>Your Favorite Student</h1>
<p><b>First Name:</b> @Model.FirstName</p>
<p><b>Last Name:</b> @Model.LastName</p>
<p><b>Student ID:</b> @Model.StudentId</p>
<p><b>Email Address:</b> @Model.EmailAddress</p>
<p><b>Description Of Student:</b> @Model.DescriptionOfStudent</p>
